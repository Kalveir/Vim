syntax on
set noswapfile
syntax enable
set autoindent
filetype on
filetype plugin on
filetype plugin indent on
set termguicolors
set backspace=2
set cursorline
colorscheme archman
set mouse=a
set laststatus=2
set number
set expandtab
set cursorcolumn
set confirm
let g:lightline_theme = "monokai_pro"
set clipboard=unnamedplus
